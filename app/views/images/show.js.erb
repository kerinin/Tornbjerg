$('.content[id="<%= project_image_url(@project,@image) %>"]').children().remove();
$('.content[id="<%= project_image_url(@project,@image) %>"]').addClass('image_content').append( $('<%= escape_javascript( render(:partial => 'image_content') ) %>').children() );

init_plans();

tracking_list["<%= project_image_url(@project,@image) %>"] = [
  ['_trackEvent', 'Projects', 'View', '<%= @project.name %>'],
  ['_trackEvent', 'Image', 'View', '<%= @project.name %>/<%= @image.id %>']
];

