-content_for :head do
  %title= "#{@project.name} - Videos  (#{ @video.position }) @ Bercy Chen Studio: Architecture + Construction"

  %meta{ :name => "keywords", :content => "video #{ @project.tags.map(&:name).join(' ') } #{ @project.name }" }
  -unless @video.description.empty?
    %meta{ :name => "description", :content => @video.description  }

  = render :partial => 'shared/prerender_support'
    
-content_for :nav_left do
  = render :partial => 'shared/project_show_nav'

= render :partial => 'shared/project_thumbnails'
	
= render :partial => 'video_content'
	
-content_for :footer do
  = render :partial => 'shared/project_footer'


